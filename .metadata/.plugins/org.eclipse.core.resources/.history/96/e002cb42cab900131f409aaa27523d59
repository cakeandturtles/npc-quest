package com.meow.kittypaintdev.server;

import java.awt.image.BufferedImage;;

public class DrawrServerChunk {
	public DrawrServerChunk(DrawrServerMap drawr_map){
		//Create image not canvas
		BufferedImage img = new BufferedImage(drawr_map.chunk_block_size, 
				drawr_map.chunk_block_size, BufferedImage.TYPE_INT_ARGB);
	}
}
