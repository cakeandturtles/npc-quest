package Managers;

import java.awt.event.KeyEvent;
import java.util.HashMap;

public class KeyManager {
	public HashMap<Integer, Boolean> keys_down;
	public HashMap<Integer, Boolean> keys_pressed;
	public HashMap<Integer, Boolean> keys_have_pressed;
	public HashMap<Integer, Boolean> keys_up;
	
	public KeyManager(){
		keys_down = new ArrayList<Integer>();
		keys_pressed = new ArrayList<Integer>();
		keys_have_pressed = new ArrayList<Integer>();
		keys_up = new ArrayList<Integer>();
	}
	
	public void KeyDown(KeyEvent e){
		keys_down
	}
}
