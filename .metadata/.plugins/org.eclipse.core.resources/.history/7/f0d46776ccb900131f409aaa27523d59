package com.meow.kittypaintdev.server;

import java.awt.Image;

public class BrushObj {
	Image img;
	String name;
	int size;
	Image[] sized_images;
	String type;
	boolean loaded;
	
	public BrushObj(Image img, String name, int size, String type){
		this.img = img;
		this.name = name;
		this.size = size;
		this.sized_images = null;
		this.type = type;
		this.loaded = false;
	}
}
