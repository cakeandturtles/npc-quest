import javax.swing.JFrame;

@SuppressWarnings("serial")
public class Main extends JFrame{
	public Main(){
		add(new GameWindow());
		
		setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
		setSize(640, 480);
		setLocationRelativeTo(null);
		setTitle("Grimoire");
		setResizable(false);
		setVisible(true);
	}
	
	public static void main(String[] args){
		new Main();
	}
}
